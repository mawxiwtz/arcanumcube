{
  "name": "arcanumcube",
  "version": "0.0.1",
  "description": "Arcanum Cube",
  "private": true,
  "type": "module",
  "license": "MIT",
  "directories": {
    "dist": "./dist/"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/lib/cjs/arcanumcube.js",
  "module": "./dist/lib/esm/arcanumcube.js",
  "exports": {
    ".": {
      "import": "./dist/lib/esm/arcanumcube.js",
      "require": "./dist/lib/cjs/arcanumcube.js",
      "types": "./dist/lib/types/arcanumcube.d.ts"
    }
  },
  "scripts": {
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "build": "node ./esbuild.js",
    "postbuild": "tsc --allowJs --declaration --emitDeclarationOnly --outDir ./dist/lib/types",
    "start": "node ./esbuild.js --serve",
    "doc": "typedoc --entryPointStrategy expand ./src --out ./docs --disableSources",
    "test": "jest"
  },
  "devDependencies": {
    "@tweakpane/core": "^2.0.3",
    "@types/eslint": "^8.56.10",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/three": "^0.163.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "chalk": "^5.3.0",
    "esbuild": "^0.20.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.1",
    "ts-jest": "^29.1.4",
    "typedoc": "^0.25.13",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.20.0",
    "@tweakpane/plugin-essentials": "^0.2.1",
    "@tweenjs/tween.js": "^23.1.2",
    "three": "^0.163.0",
    "tweakpane": "^4.0.3"
  }
}
